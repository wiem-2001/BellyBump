{% extends 'back.html.twig' %}

{% block title %}Doctors{% endblock %}

{% block body %}
    <h1>Doctors</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ med.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ med.nom }}</td>
            </tr>
            <tr>
                <th>FullName</th>
